pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Test') {
            steps {
                echo 'TEST Hello World'
            }
        }
       stage('Deploy') {
            steps {
                echo 'DEPLOY Hello World -- AFTER GITHUB Webhook'
            }
        }
    }
}
